//
// compile to *.res with:
//   rc.exe /r "$(ProjectDir)mmconfig.rc"
//

1                       ICON                    "MegaMol_Ctrl.ico"
2                       ICON                    "MegaMol_Proj.ico"
3                       ICON                    "MegaMol_Plg.ico"
1                       RT_MANIFEST             "mmconfig.manifest"

// Version
1 VERSIONINFO
 FILEVERSION ${MEGAMOL_MAJOR},${MEGAMOL_MINOR},${MEGAMOL_PATCH},${GIT_HASH}
 PRODUCTVERSION ${MEGAMOL_MAJOR},${MEGAMOL_MINOR},${MEGAMOL_PATCH},${GIT_HASH}
 FILEFLAGSMASK 0x3fL
#if defined(_DEBUG) || defined(DEBUG)
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040704b0"
        BEGIN
            VALUE "CompanyName", "${PROJ_TEAM}"
            VALUE "FileDescription", "${PROJ_DESC}"
            VALUE "FileVersion", "${MEGAMOL_MAJOR}.${MEGAMOL_MINOR}.${MEGAMOL_PATCH}.${GIT_HASH}"
            VALUE "InternalName", "MegaMol Configurator"
            VALUE "LegalCopyright", "Copyright (C) 2013-${YEAR}"
            VALUE "OriginalFilename", "MegaMolConf.exe"
            VALUE "ProductName", "${PROJ_NAME}"
            VALUE "ProductVersion", "${MEGAMOL_MAJOR}.${MEGAMOL_MINOR}.${MEGAMOL_PATCH}.${GIT_HASH}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x407, 1200
    END
END
