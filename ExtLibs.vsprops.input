<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
  ProjectType="Visual C++"
  Version="8.00"
  Name="ExtLibs"
  >
  <Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="%withNETCDF%!$(NETCDFPath)\include, !! %withOPENCL%!$(OPENCLPath)\OpenCL\common\inc, !!"
		PreprocessorDefinitions="%withNETCDF%!WITH_NETCDF, !! %withOPENCL%!WITH_OPENCL, !!"
	/>
  <Tool
		Name="VCLinkerTool"
		AdditionalLibraryDirectories="%withNETCDF%!$(NETCDFPath)\lib$(BitsD), !! %withOPENCL%!$(OPENCLPath)\OpenCL\common\lib\x64!!"
		AdditionalDependencies="%withNETCDF%!netcdf$(Bits).lib!! %withOPENCL%!OpenCL.lib!!"
	/>
  <UserMacro
  Name="OutputBin"
  Value="%outbin%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
  Name="MegaMolCorePath"
  Value="%mmcorePath%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
  Name="VISlibPath"
  Value="%vislib%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
		Name="NETCDFPath"
		Value="%netcdfPath%"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="OPENCLPath"
		Value="%openclPath%"
		PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>
