<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
  ProjectType="Visual C++"
  Version="8.00"
  Name="ExtLibs"
  >
  <Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="%withNETCDF%!$(NETCDFPath)\include!!"
		PreprocessorDefinitions="%withNETCDF%!WITH_NETCDF!!"
	/>
  <Tool
		Name="VCLinkerTool"
		AdditionalLibraryDirectories="%withNETCDF%!$(NETCDFPath)\lib$(BitsD)!!"
		AdditionalDependencies="%withNETCDF%!netcdf$(Bits).lib!!"
	/>
  <UserMacro
  Name="OutputBin"
  Value="%outbin%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
  Name="OutputShaders"
  Value="%outshader%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
  Name="MegaMolCorePath"
  Value="%mmcorePath%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
  Name="VISlibPath"
  Value="%vislib%"
  PerformEnvironmentSet="true"
  />
  <UserMacro
		Name="NETCDFPath"
		Value="%netcdfPath%"
		PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>
