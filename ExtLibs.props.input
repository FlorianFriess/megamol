<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <OutputBin>%outbin%</OutputBin>
    <MegaMolCorePath>%mmcorePath%</MegaMolCorePath>
    <VISlibPath>%vislib%</VISlibPath>
    <HAPTICSPath>%hapticsPath%</HAPTICSPath>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>%withHAPTICS%!$(HAPTICSPath)/include;$(HAPTICSPath)/utilities/include;!!%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>%withCUDA%!WITH_CUDA;!!%withHAPTICS%!WITH_OPENHAPTICS;!!%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>%withHAPTICS%!$(HAPTICSPath)\lib\$(Platform)\$(Configuration);$(HAPTICSPath)\utilities\lib\$(Platform)\$(Configuration);$(HAPTICSPath)\lib\$(Platform);!!%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>%withCUDA%!cudart.lib;!!%withHAPTICS%!hl.lib;hlu.lib;hd.lib;hdu.lib;!!%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <CudaCompile>
      <Include>$(VISlibPath)gl\include</Include>
    </CudaCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="OutputBin">
      <Value>$(OutputBin)</Value>
    </BuildMacro>
    <BuildMacro Include="MegaMolCorePath">
      <Value>$(MegaMolCorePath)</Value>
    </BuildMacro>
    <BuildMacro Include="VISlibPath">
      <Value>$(VISlibPath)</Value>
    </BuildMacro>
    <BuildMacro Include="HAPTICSPath">
      <Value>$(HAPTICSPath)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>