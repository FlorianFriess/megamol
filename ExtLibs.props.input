<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup Label="UserMacros">
		<OutputBin>%outbin%</OutputBin>
		<MegaMolCorePath>%mmcorePath%</MegaMolCorePath>
		<VISlibPath>%vislib%</VISlibPath>
		<ANNPath>%ANNPath%</ANNPath>
	</PropertyGroup>
	<PropertyGroup>
		<_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
	</PropertyGroup>
	<ItemDefinitionGroup>
		<ClCompile>
			<AdditionalIncludeDirectories>%withANN%!$(ANNPath)include;!!%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
			<PreprocessorDefinitions>%withANN%!WITH_ANN;!!%(PreprocessorDefinitions)</PreprocessorDefinitions>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>%withANN%!$(ANNPath)lib;!!%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
			<AdditionalDependencies>%withANN%!ANN$(BitsD).lib;!!%(AdditionalDependencies)</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
	<ItemGroup>
		<BuildMacro Include="OutputBin">
			<Value>$(OutputBin)</Value>
		</BuildMacro>
		<BuildMacro Include="MegaMolCorePath">
			<Value>$(MegaMolCorePath)</Value>
		</BuildMacro>
		<BuildMacro Include="VISlibPath">
			<Value>$(VISlibPath)</Value>
		</BuildMacro>
		<BuildMacro Include="ANNPath">
			<Value>$(ANNPath)</Value>
		</BuildMacro>
	</ItemGroup>
</Project>