<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <OutputBin>%outbin%</OutputBin>
    <MegaMolCorePath>%mmcorePath%</MegaMolCorePath>
    <VISlibPath>%vislib%</VISlibPath>
	<ProteinCallsPath>%protein_calls%</ProteinCallsPath>
    <HAPTICSPath>%hapticsPath%</HAPTICSPath>
	<OPENBABELPath>%openbabelPath%</OPENBABELPath>
	<CURLPath>%curlPath%</CURLPath>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>%withHAPTICS%!$(HAPTICSPath)/include;$(HAPTICSPath)/utilities/include;!!%withOPENBABEL%!$(OPENBABELPath)/include;!!%withCURL%!$(CURLPath)\include;!!%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>%withCUDA%!WITH_CUDA;!!%withHAPTICS%!WITH_OPENHAPTICS;!!%withOPENBABEL%!WITH_OPENBABEL;!!%withCURL%!WITH_CURL;!!%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>%withHAPTICS%!$(HAPTICSPath)\lib\$(Platform)\$(Configuration);$(HAPTICSPath)\utilities\lib\$(Platform)\$(Configuration);$(HAPTICSPath)\lib\$(Platform);!!%withOPENBABEL%!$(OPENBABELPath)\lib\$(Platform)\$(Configuration);!!%withCURL%!$(CURLPath)\lib\$(Platform)\$(Configuration);!!%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>%withCUDA%!cudart.lib;!!%withHAPTICS%!hl.lib;hlu.lib;hd.lib;hdu.lib;!!%withOPENBABEL%!openbabel-2.lib;!!%withCURL%!libcurl.lib;!!%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <CudaCompile>
      <Include>$(VISlibPath)gl\include</Include>
    </CudaCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="OutputBin">
      <Value>$(OutputBin)</Value>
    </BuildMacro>
    <BuildMacro Include="MegaMolCorePath">
      <Value>$(MegaMolCorePath)</Value>
    </BuildMacro>
    <BuildMacro Include="VISlibPath">
      <Value>$(VISlibPath)</Value>
    </BuildMacro>
	<BuildMacro Include="ProteinCallsPath">
      <Value>$(ProteinCallsPath)</Value>
    </BuildMacro>
    <BuildMacro Include="HAPTICSPath">
      <Value>$(HAPTICSPath)</Value>
    </BuildMacro>
	<BuildMacro Include="OPENBABELPath">
	  <Value>$(OPENBABELPath)</Value>
    </BuildMacro>
	<BuildMacro Include="CURLPath">
		<Value>$(CURLPath)</Value>
	</BuildMacro>
  </ItemGroup>
</Project>