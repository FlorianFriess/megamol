<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <OutputBin>%outbin%</OutputBin>
    <MegaMolCorePath>%mmcorePath%</MegaMolCorePath>
    <VISlibPath>%vislib%</VISlibPath>
    <NETCDFPath>%netcdfPath%</NETCDFPath>
    <OPENCLPath>%openclPath%</OPENCLPath>
    <CUDAPath>%cudaPath%</CUDAPath>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>%withNETCDF%!$(NETCDFPath)\include;!!%withOPENCL%!$(OPENCLPath)\OpenCL\common\inc;!!%withCUDA%!$(CUDA_INC_PATH);$(CUDAPath)/C/common/inc;!!;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>%withNETCDF%!WITH_NETCDF, !!%withOPENCL%!WITH_OPENCL, !!%withCUDA%!WITH_CUDA, !!;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>%withNETCDF%!$(NETCDFPath)\lib$(BitsD);!!%withOPENCL%!$(OPENCLPath)\OpenCL\common\lib\x64;!!%withCUDA%!$(CUDA_LIB_PATH);$(CUDAPath)/C/common/lib;!!;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>%withNETCDF%!netcdf$(Bits).lib !!%withOPENCL%!OpenCL.lib !!%withCUDA%!cudart.lib cutil$(BitsD).lib cudpp$(BitsD).lib cuda.lib !!;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="OutputBin">
      <Value>$(OutputBin)</Value>
    </BuildMacro>
    <BuildMacro Include="MegaMolCorePath">
      <Value>$(MegaMolCorePath)</Value>
    </BuildMacro>
    <BuildMacro Include="VISlibPath">
      <Value>$(VISlibPath)</Value>
    </BuildMacro>
    <BuildMacro Include="NETCDFPath">
      <Value>$(NETCDFPath)</Value>
    </BuildMacro>
    <BuildMacro Include="OPENCLPath">
      <Value>$(OPENCLPath)</Value>
    </BuildMacro>
    <BuildMacro Include="CUDAPath">
      <Value>$(CUDAPath)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>